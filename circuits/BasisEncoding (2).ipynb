   "source": [
    "from qiskit import QuantumCircuit\n",
    "\n",
    "\n",
    "input = 7\n",
    "circuits = len(bin(input))\n",
    "qc = QuantumCircuit(circuits - 2)\n",
    "for i in range(0, circuits - 2):\n",
    "    initial = int(bin(input)[i + 2])\n",
    "    qc.initialize(initial, i)\n",
    "print(qc)"
   ]
